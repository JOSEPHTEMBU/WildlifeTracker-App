{{#partial "content"}}
    <div class="container">
        <h5 class="text-center">Adding Locations</h5>
        <hr>
        <form method="Post" action="/addlocation">
            <div class="form-group">
                <label for="name">Name of the Location Visited</label>
                <input type="text" class="form-control" id="name" name="name" placeholder="Location Name">
            </div>

            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
{{/partial}}

{{> format.hbs}}
